              }
            },
            {
              "properties": {
                "teacher": {
                  "$ref": "#/components/schemas/TeacherShort"
                }
              }
            }
          ],
          "role": {
            "type": "string",
            "description": "The role of the user.",
            "enum": ["STUDENT", "TEACHER"]
          },
          "classes": {
            "type": "array",
            "description": "The classes that a student is in",
